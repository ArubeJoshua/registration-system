<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                                  http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <!--    Beans
    -->

    <managed-bean>
        <managed-bean-name>loginBean</managed-bean-name>
        <managed-bean-class>org.pahappa.systems.registrationapp.views.LoginBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>cacheControlBean</managed-bean-name>
        <managed-bean-class>org.pahappa.systems.registrationapp.views.CacheControlBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>dependantBean</managed-bean-name>
        <managed-bean-class>org.pahappa.systems.registrationapp.views.DependantBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>registerBean</managed-bean-name>
        <managed-bean-class>org.pahappa.systems.registrationapp.views.RegisterBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>userBean</managed-bean-name>
        <managed-bean-class>org.pahappa.systems.registrationapp.views.UserBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>adminBean</managed-bean-name>
        <managed-bean-class>org.pahappa.systems.registrationapp.views.AdminBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>userHomeBean</managed-bean-name>
        <managed-bean-class>org.pahappa.systems.registrationapp.views.UserHomeBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>

</faces-config>